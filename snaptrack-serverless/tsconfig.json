{
    "compilerOptions": {
      "target": "ES5",
      "module": "commonjs",
      "lib":  ["dom", "es5", "scripthost"],
      "strict": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "strictFunctionTypes": true,
      "strictBindCallApply": true,
      "strictPropertyInitialization": true,
      "noImplicitThis": true,
      "alwaysStrict": true,
      "esModuleInterop": true
    }
  }